<%= render 'error' %>

<%= form_for :login, url: authenticate_path, method: 'post' do |f| %>
  <%= f.label :username %>: <%= f.text_field :username %><br />
  <%= f.label :password %>: <%= f.password_field :password %><br />
  <%= f.submit "Login" %>
<% end %>

<%= link_to "Create an Account", new_designer_path %>
