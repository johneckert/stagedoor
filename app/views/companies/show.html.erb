<h1>Analytics for <%= @company.name %></h1>
<% @venues.each_with_index do |ven, i| %>
  <div class="venue_data">
    <h2><%= ven.name %></h2>
    <div class="fee_data">
      <p>Average Fee: <%= ven.mean_fee %> | Lowest Fee: <%= ven.min_fee %> | Highest Fee: <%= ven.max_fee %> | Median Fee: <%= ven.median_fee %> | Most Frequent: <%= ven.mode_fee %></p>
    </div>
      <div id="chart <%= ven.id %>"></div>
      <%= render_chart(@chart, "chart #{ven.id}") %>
    </div>
<% end %>
